	<xsd:simpleType name="trueFalseType">
		<xsd:annotation>
			<xsd:documentation>This type defines two different values which can
				designate boolean values. </xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="true" />
			<xsd:enumeration value="false" />
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:complexType name="JAZN-DATAstringType">
		<xsd:simpleContent>
			<xsd:extension base="xsd:string">
				<xsd:attribute name="locale" type="xsd:string" />
			</xsd:extension>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="JAZN-DATAextended-attributesType">
		<xsd:annotation>
			<xsd:documentation>
				This element specifies an Attribute type.
           </xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="attribute" type="JAZN-DATAextended-propertiesType"
				maxOccurs="unbounded" />
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="JAZN-DATAextended-propertiesType">
		<xsd:annotation>
			<xsd:documentation>
				This element specifies an extended properties type.
           </xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="name" type="xsd:string" />
			<xsd:element name="values" type="JAZN-DATAextended-property-valuesType" />
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="JAZN-DATAextended-property-valuesType">
		<xsd:annotation>
			<xsd:documentation>
				This element specifies a values type.
            </xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="value" type="JAZN-DATAextended-property-valueType" maxOccurs="unbounded" />
		</xsd:sequence>
	</xsd:complexType>
        <xsd:complexType name="JAZN-DATAextended-property-valueType">
                <xsd:annotation>
                        <xsd:documentation>
                                This element specifies a value type.
            </xsd:documentation>
                </xsd:annotation>
                <xsd:simpleContent>
                    <xsd:extension base="xsd:string">
                       <xsd:attribute name="metadata-member-type" type="xsd:string" />
                       <xsd:attribute name="owner-version" type="xsd:string" />
                    </xsd:extension>
                </xsd:simpleContent>
        </xsd:complexType>
	<xsd:complexType name="JAZN-DATAapprolepoliciesType">
