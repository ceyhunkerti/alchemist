		<xsd:sequence>
			<xsd:element name="attribute" type="JAZN-DATAattributesType"
				minOccurs="1" />
			<xsd:element name="matchingFunction" type="xsd:string"
				minOccurs="1" />
			<xsd:element name="mustBePresent" type="xsd:boolean"
				minOccurs="1" />
			<xsd:element name="attributeValue" type="JAZN-DATAattributeValueType"
				minOccurs="1" />
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="JAZN-DATAattributeValueType">
		<xsd:sequence>
			<xsd:element name="value" type="xsd:string" minOccurs="0"
