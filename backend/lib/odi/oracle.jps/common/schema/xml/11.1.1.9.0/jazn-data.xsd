	<xsd:complexType name="JAZN-DATAprincipalsType">
		<xsd:annotation>
			<xsd:documentation>
				This element specifies a Principal.
            </xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="principal" type="JAZN-DATAprincipalType"
				minOccurs="0" maxOccurs="unbounded" />
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="JAZN-DATAprincipalType">
		<xsd:annotation>
			<xsd:documentation>
				This element specifies a principal.
            </xsd:documentation>
		</xsd:annotation>
		<xsd:all>
			<xsd:element name="name" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>
						This element specifies the name of the principal.
                    </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="class" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>
						This element specifies the full qualified name of the
						Java class implementing the Principal.
                    </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="uniquename" type="xsd:string"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						This element specifies the unique name of the principal.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="guid" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						This element specifies a globally unique identifier of
						the principal.
                    </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
                    <xsd:element name="extended-attributes" type="JAZN-DATAextended-attributesType" minOccurs="0" maxOccurs="1">
                        <xsd:annotation>
                            <xsd:documentation>This is the properties for a principal.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
		</xsd:all>
	</xsd:complexType>
	<xsd:complexType name="JAZN-DATAcodesourceType">
		<xsd:annotation>
			<xsd:documentation>
				This element specifies a codesource.
            </xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="url" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>
						This element specifies a codesource URL that is used as
						a codesource.
                    </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="JAZN-DATApermissionsType">
		<xsd:annotation>
			<xsd:documentation>
				This element specifies a Java Permission.
            </xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="permission" type="JAZN-DATApermissionType"
				maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>
						This element specifies a permission.
                    </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="JAZN-DATApermissionType">
		<xsd:all>
		        <xsd:element name="metadata-member-type" type="xsd:string" minOccurs="0" maxOccurs="1"/>
		        <xsd:element name="owner-version" type="xsd:string" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="class" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>
						This element specifies a full qualified name of a class
						that implements this permission.
                    </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="name" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						This element specifies the name of the permission.
                    </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="actions" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						This element specifies the actions of the permission.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="description" type="JAZN-DATAstringType"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation />
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="display-name" type="JAZN-DATAstringType"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation />
				</xsd:annotation>
			</xsd:element>
		</xsd:all>
	</xsd:complexType>
	<xsd:complexType name="JAZN-DATAjazn-permission-classesType">
		<xsd:sequence>
			<xsd:element name="permission-class" type="JAZN-DATApermission-classType"
				minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>
						RESERVED FOR FUTURE USE
                    </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="JAZN-DATApermission-classType">
		<xsd:annotation>
			<xsd:documentation>
				RESERVED FOR FUTURE USE
            </xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="name" type="xsd:string" />
			<xsd:element name="description" type="xsd:string"
				minOccurs="0" />
			<xsd:element name="type" type="xsd:string" />
			<xsd:element name="class" type="xsd:string" />
			<xsd:element name="target-descriptors" type="JAZN-DATAtarget-descriptorsType" />
			<xsd:element name="action-descriptors" type="JAZN-DATAaction-descriptorsType"
				minOccurs="0" />
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="JAZN-DATAtarget-descriptorsType">
		<xsd:annotation>
			<xsd:documentation>
				RESERVED FOR FUTURE USE
            </xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="target-descriptor" type="JAZN-DATAtarget-descriptorType"
				minOccurs="0" maxOccurs="unbounded" />
