	</xsd:complexType>
	<xsd:complexType name="JAZN-DATAtargetresourceType">
		<xsd:all>
			<xsd:element name="name" type="xsd:string" />
			<xsd:element name="type-name-ref" type="xsd:string" />
                        <xsd:element name="metadata-member-type" type="xsd:string" minOccurs="0" maxOccurs="1"/>
                        <xsd:element name="owner-version" type="xsd:string" minOccurs="0" maxOccurs="1"/>
		</xsd:all>
	</xsd:complexType>
	<xsd:complexType name="JAZN-DATAtargetresourceexpressionsType">
		<xsd:sequence>
			<xsd:element name="target-resource-expression" type="JAZN-DATAtargetresourceexpressionType"
				minOccurs="0" maxOccurs="unbounded" />
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="JAZN-DATAtargetresourceexpressionType">
		<xsd:all>
			<xsd:element name="resource-expression" type="xsd:string" />
			<xsd:element name="type-name-ref" type="xsd:string" />
                        <xsd:element name="metadata-member-type" type="xsd:string" minOccurs="0" maxOccurs="1"/>
                        <xsd:element name="owner-version" type="xsd:string" minOccurs="0" maxOccurs="1"/>
		</xsd:all>
	</xsd:complexType>
	<xsd:complexType name="JAZN-DATAobligationType">
		<xsd:sequence>
			<xsd:element name="obligationExpression" type="xsd:string"
				minOccurs="0" />
