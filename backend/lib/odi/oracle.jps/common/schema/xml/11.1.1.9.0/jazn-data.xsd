			<xsd:element name="dataType" type="xsd:string" minOccurs="1" />
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="JAZN-DATAmetaDataType">
		<xsd:annotation>
			<xsd:documentation>
				This element specifies the meta-data information for a policy artifact
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="owner" type="xsd:string" minOccurs="1"
				maxOccurs="1" />
			<xsd:element name="owner-version" type="xsd:string"
				minOccurs="1" maxOccurs="1" />
			<xsd:element name="modifier" type="xsd:string" minOccurs="1"
				maxOccurs="1" />
			<xsd:element name="customized" type="xsd:boolean"
				minOccurs="1" maxOccurs="1" />
			<xsd:element name="custom-version" type="xsd:string"
				minOccurs="1" maxOccurs="1" />
			<xsd:element name="soft-delete" type="xsd:boolean"
				minOccurs="1" maxOccurs="1" />
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="JAZN-DATAsystem-policyType">
		<xsd:sequence>
			<xsd:element name="admin-policy" type="JAZN-DATAadmin-policyType"
				minOccurs="0" />
			<xsd:element name="jazn-policy" type="JAZN-DATAjazn-policyType"
				minOccurs="0" />
		</xsd:sequence>		
	</xsd:complexType>
</xsd:schema>
