	<xsd:complexType name="JAZN-DATApolicyCombinerParameterType">
		<xsd:complexContent>
			<xsd:extension base="JAZN-DATAcombinerParameterType">
				<xsd:attribute name="policyId" type="xsd:string" use="required" />
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="JAZN-DATApolicySetCombinerParameterType">
		<xsd:complexContent>
			<xsd:extension base="JAZN-DATAcombinerParameterType">
				<xsd:attribute name="policySetId" type="xsd:string"
					use="required" />
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="JAZN-DATApolicyIssuerType">
		<xsd:sequence>
			<xsd:element name="attribute" type="JAZN-DATAattributesType"
				minOccurs="0" maxOccurs="unbounded" />
			<xsd:element name="content" type="xsd:string" minOccurs="0" />
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="JAZN-DATAconstraintType">
		<xsd:annotation>
			<xsd:documentation>
				All AVAs are ANDed in a constraint.
            </xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="attributeValueAssertion" type="JAZN-DATAattributeValueAssertionType"
				minOccurs="1" maxOccurs="unbounded" />
		</xsd:sequence>
	</xsd:complexType>
