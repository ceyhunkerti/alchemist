		<xsd:attribute name="obligationId" type="xsd:string"
			use="required" />
		<xsd:attribute name="effectType" type="xsd:string" use="required" />
	</xsd:complexType>
	<xsd:complexType name="JAZN-DATAadviceType">
		<xsd:sequence>
			<xsd:element name="adviceExpression" type="xsd:string"
				minOccurs="0" />
		</xsd:sequence>
		<xsd:attribute name="adviceId" type="xsd:string" use="required" />
		<xsd:attribute name="appliesTo" type="xsd:string" use="required" />
	</xsd:complexType>
	<xsd:complexType name="JAZN-DATAcombinerParameterType">
		<xsd:sequence>
			<xsd:element name="attributeValue" type="JAZN-DATAattributeValueType"
				minOccurs="0" maxOccurs="unbounded" />
